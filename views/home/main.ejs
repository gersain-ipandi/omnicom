	<script>
		// Calcul automatique du prix de transport de la marchandise
		var btQuoteTotalCalc = document.getElementById('btQuoteTotalCalc');
		var inputKiloId = document.getElementById('inputKiloId');
		// Fonction d'ecoute de changement sur le champ du poids de marchandise
		inputKiloId.addEventListener('change', (val) => {
			calculPrixKiloAuto(inputKiloId.value);
		});
		// Fonction d'ecoute de saisie sur le champ du poids de marchandise
		inputKiloId.addEventListener('keyup', (val) => {
			calculPrixKiloAuto(inputKiloId.value);
		});
		// Fonction de calcul
		function calculPrixKiloAuto(kilos) {
			var prixUnKilo = 300; //Prix franc cfa
			var total = kilos * prixUnKilo;

			btQuoteTotalCalc.innerText = total;
		}

	</script>
